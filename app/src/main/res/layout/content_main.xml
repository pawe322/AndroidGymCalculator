<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    tools:showIn="@layout/app_bar_main">

    <TextView
        android:id="@+id/RM8string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/RM5string"
        android:layout_below="@+id/RM6string"
        android:text="8RM"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM12string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/RM8string"
        android:layout_below="@+id/RM10string"
        android:text="12RM"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM10string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/RM8string"
        android:layout_below="@+id/RM8string"
        android:text="10RM"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="104dp"
        android:layout_height="44dp"
        android:layout_below="@+id/seekBar1"
        android:layout_centerHorizontal="true" />

    <TextView
        android:id="@+id/RM5string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/RM1string"
        android:layout_below="@+id/RM1string"
        android:text="5RM"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM6string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/RM5string"
        android:layout_below="@+id/RM5string"
        android:text="6RM"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM1string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="43dp"
        android:text="1RM"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM1"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM5"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/RM5string"
        android:layout_centerHorizontal="true"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM6"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/RM8string"
        android:layout_centerHorizontal="true"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM8"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/RM8string"
        android:layout_centerHorizontal="true"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM10"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RM8string"
        android:layout_centerHorizontal="true"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/RM12"
        android:layout_width="87dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/RM12string"
        android:layout_centerHorizontal="true"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="100dp"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="251dp"
        android:layout_height="44dp"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <SeekBar
        android:id="@+id/seekBar1"
        android:layout_width="251dp"
        android:layout_height="44dp"
        android:layout_below="@+id/textView1"
        android:layout_centerHorizontal="true" />

</RelativeLayout>